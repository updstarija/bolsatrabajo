            </form>
            @{OcultarForm = Model.Educacion.Idioma.Count() == 1 ? "" : "d-none";}
            <div class="d-flex justify-content-start">
                <span id="spnEliminarIS" class="badge badge-pill span-red puntero @OcultarForm" onclick="OcultarForm('formIS','needs-validation-formIS','divAddIS','agregarIS0','agregarIS','spnEliminarIS')">Eliminar</span>
            </div>
            @{OcultarForm = Model.Educacion.Idioma.Count() == 0 ? "d-none" : "";}
            <div class="d-flex justify-content-center">
                <span id="agregarIS" class="badge badge-pill span-blue @OcultarForm" style="cursor:pointer;" onclick="AgregarCampos('formIS','ClonIS','needs-validation-formIS','spnEliminarIS')">Agregar campo +</span>
            </div>
        </div>
        <div class="tab-pane fade p-3" id="Informacion_General" role="tabpanel" aria-labelledby="ig-tab">
            <h1>Informacion General</h1>
            <form id="formIG" class="needs-validation-formIG my-2" novalidate>
                <div class="form-row">
                    <div class="form-row col-12 mx-0 px-0">
                        <div class="col-12 col-sm-6">
                            <label for="tituloIG">Titulo:</label>
                            <input type="text" class="form-control" id="tituloIG" name="tituloIG" value="@Model.Titulo" required>
                            <div class="invalid-feedback">¡Campo Vacio!</div>
                            <input type="hidden" id="idIG" name="idIG" value="@Model.Id">
                        </div>
                        <div class="col-12 col-sm-3">
                            <label for="contratoIG">Contrato:</label>
                            <select class="custom-select" id="contratoIG" name="contratoIG" value="@Model.Contrato" required>
                                <option disabled value="">Cambiar...</option>
                                <option value="Tiempo Completo">Tiempo Completo</option>
                                <option value="Medio Tiempo">Medio Tiempo</option>
                                <option value="Otro">Otro</option>
                            </select>
                            <div class="invalid-feedback">
                                Por favor seleccione un tipo.
                            </div>
                        </div>
                        <div class="col-12 col-sm-3">
                            <label for="pretencionSalarialIG">Pretencion Salarial:</label>
                            <input type="number" class="form-control" id="pretencionSalarialIG" name="pretencionSalarialIG" min="1" pattern="^[0-9]+" value="@Model.PretencionSalarial" required>
                            <div class="invalid-feedback">¡Campo Vacio!</div>
                        </div>
                        <div class="col-12 col-sm-6 mt-3">
                            <label for="IdCarrera">Carrera:</label>
                            <select class="custom-select" id="IdCarrera" name="IdCarrera" size="5" required>
                                <option disabled value="">Seleccionar Carrera...</option>
                                @foreach (var item in ViewBag.tCarreras)
                                {
                                    <option value="@item.Id">@item.Nombre</option>
                                }
                            </select>
                            <div class="invalid-feedback">¡Campo Vacio!</div>
                        </div>
                        <div class="col-12 col-sm-3">
                            <div class="mt-3">
                                <label for="paisIG">Pais:</label>
                                <input type="text" class="form-control" id="paisIG" name="paisIG[]" value="@Model.PaisResidencia" required>
                                <div class="invalid-feedback">¡Campo Vacio!</div>
                            </div>
                            <div class="mt-1">
                                <label for="ciudadIG">Ciudad</label>
                                <input type="text" class="form-control" id="ciudadIG" name="ciudadIG[]" value="@Model.Ciudad" required>
                                <div class="invalid-feedback">¡Campo Vacio!</div>
                            </div>
                        </div>
                        <div class="col-12 col-sm-3 mt-3">
                            <label for="estadoRegionIG">Estado o Region:</label>
                            <input type="text" class="form-control" id="estadoRegionIG" name="estadoRegionIG[]" value="@Model.EstadoRegion" required>
                            <div class="invalid-feedback">¡Campo Vacio!</div>
                        </div>
                    </div>
                    <div class="form-row col-12 mx-0 px-0">
                        <div class="col-12 col-sm-12 mt-3">
                            <label for="presentacionBiografiaIG">Presentacion/Biografia</label>
                            <p for="descripcionIG" style="font-size:13px;">Por favor verifique ortografía y gramática y no escriba el texto en mayusculas ya que esto puede hacer que su currículo sea rechazado para ser incluido en nuestra base de datos de curriculos.</p>
                            <textarea class="form-control" id="presentacionBiografiaIG" name="presentacionBiografiaIG[]" minlength="10" maxlength="500" style="resize: none; height: 150px;" required onkeyup="ContarCaracteres($(this).val(),'500',$(this))">@Model.PresentacionBiografica</textarea>
                            <p id="CaracteresDisponibles" class="text-left"></p>
                            <div class="invalid-feedback">¡Campo Vacio!</div>
                        </div>
                    </div>
                    <div class="form-row col-12 mx-0 px-0">
                        <div class="col-12 col-sm-12 mt-3">
                            <label for="privacidadIG">Privacidad</label>
                            <select class="custom-select" id="privacidadIG" name="privacidadIG" required>
                                <option disabled value="">Cambiar...</option>
                                <option value="0">Solamente a las empresas que postulo</option>
                                <option value="1">Publico</option>
                            </select>
                            <div class="invalid-feedback">
                                Por favor seleccione un tipo.
                            </div>
                        </div>
                    </div>
                </div>
            </form>
        </div>
        <div class="form-group px-3 py-2">
            <button class="btn btn-Blue btn-block" type="submit" form="formDP">Guardar</button>
        </div>
    </div>
</div>