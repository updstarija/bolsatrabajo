                                    <option id="Basico">Basico</option>
                                    <option id="Medio">Medio</option>
                                    <option id="Avanzado">Avanzado</option>
                                </select>
                                <div class="invalid-feedback">
                                    Por favor seleccione un tipo.
                                </div>
                            </div>
                        </div>
                        <div class="form-row col-12 mx-0 px-0">
                            <div class="col-12">
                                <span id="spnEliminar" class="badge badge-pill span-red puntero ocultar" onclick="EliminarCampos($(this),'formIS','spnEliminarIS')">Eliminar</span>
                            </div>
                        </div>
                        <div class="col-11 mx-auto py-1" style="border-bottom: 1px solid #B6C2CB;">
                        </div>
                    </div>
