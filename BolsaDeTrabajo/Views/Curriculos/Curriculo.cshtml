                            <div class="invalid-feedback">¡Seleccione una fecha!</div>
                        </div>
                    </div>
                </div>
            </form>
            <h1>Formación</h1>
            <div id="divAddES" class="d-flex justify-content-center">
                <span id="agregarES0" class="badge badge-pill span-blue" style="cursor:pointer;" onclick="MostrarForm('formES','needs-validation-formES','divAddES','agregarES0','agregarES','spnEliminarES')">Agregar campo +</span>
            </div>
            <form id="formES" class="needs-validation-formES my-2 d-none" novalidate>
                <div id="Clon" class="form-row my-2 ClonES">
                    <div class="form-row col-12 mx-0 px-0">
                        <div class="col-12 col-sm-6 mt-3">
                            <label for="universidadInstitucionES">Universidad o Institución</label>
                            <input type="text" class="form-control" id="universidadInstitucionES" name="universidadInstitucionES[]" required>
                            <div class="invalid-feedback">¡Campo Vacio!</div>
                            <input type="hidden" id="idES" name="idES" value="-1">
                        </div>
                        <div class="col-12 col-sm-6 mt-3">
                            <label for="ccsES">Carrera, Curso o Seminario</label>
                            <input type="text" class="form-control" id="ccsES" name="ccsES[]" required>
                            <div class="invalid-feedback">¡Campo Vacio!</div>
                        </div>
                    </div>
                    <div class="form-row col-12 mx-0 px-0">
                        <div class="col-12 col-sm-3 mt-3">
                            <label for="nivelEstudioES">Nivel de Estudio</label>
                            <input type="text" class="form-control" id="nivelEstudioES" name="nivelEstudioES[]" required>
                            <div class="invalid-feedback">¡Campo Vacio!</div>
                        </div>
                        <div class="col-12 col-sm-3 mt-3">
                            <label for="paisES">Pais:</label>
                            <input type="text" class="form-control" id="paisES" name="paisES[]" required>
                            <div class="invalid-feedback">¡Campo Vacio!</div>
                        </div>
                        <div class="col-12 col-sm-3 mt-3">
                            <label for="estadoRegionES">Estado o Region:</label>
                            <input type="text" class="form-control" id="estadoRegionES" name="estadoRegionES[]" required>
                            <div class="invalid-feedback">¡Campo Vacio!</div>
